generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["tracing", "metrics", "fieldReference"]
  output          = "prisma-client"
  // binaryTargets   = ["native"]
  // engineType      = "binary"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URI")
}

model Account {
  id       String @id @default(cuid())
  username String @unique
  password String
}
