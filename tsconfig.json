{
  "compilerOptions": {
    "lib": [
      "ESNext"
    ],
    "module": "NodeNext",
    "target": "ESNext",
    "moduleResolution": "nodenext",
    "moduleDetection": "auto",
    "allowImportingTsExtensions": false,
    "noEmit": false,
    //    "composite": true,
    "importHelpers": true,
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    //    "jsx": "react-jsxdev",
    "allowSyntheticDefaultImports": true,
    //    "traceResolution": true,
    //    "generateCpuProfile": ".cpuprofile",
    "skipDefaultLibCheck": true,
    //        "stripInternal": true,
    "esModuleInterop": true,
    //    "resolveJsonModule": true,
    //    "resolvePackageJsonExports": true,
    //    "resolvePackageJsonImports": true,
    //    "forceConsistentCasingInFileNames": true,
    //    "allowArbitraryExtensions": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    //    "extendedDiagnostics": true,
    "noImplicitAny": false,
    //    "tsBuildInfoFile": ".tsbuildinfo",
    //    "incremental": true,
    //    "strictFunctionTypes": true,
    "pretty": true,
    //    "sourceMap": true,
    //    "inlineSources": true,
    "allowJs": true,
    //    "diagnostics": true,
    //    "declarationMap": true,
    "outDir": "dist",
    "types": [
      "bun-types",
      "@types/node"
    ],
    "paths": {
      "prisma-client": [
        "./src/infrastructure/database/prisma/prisma-client/index.d.ts"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [],
  "ts-node": {
    "preferTsExts": true,
    "files": true,
    "pretty": true,
    "swc": true,
    "transpileOnly": true,
    "esm": false,
    "experimentalReplAwait": false,
    "experimentalResolver": false
    //    "experimentalSpecifierResolution": "node"
  }
}
