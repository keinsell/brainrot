{
  "name": "application",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:tsc": "tspc -p tsconfig.json && pnpm copy:markdown && pnpm copy:prisma",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "copy:markdown": "copyfiles -e \"**/node_modules/**\" -u 1 \"src/**/*.md\" dist",
    "copy:prisma": "copyfiles -u 1 \"src/externals/generated/prisma/**\" dist",
    "dev": "pnpm run dev:tsc",
    "dev:tsc": "nodemon",
    "dev:nest": "nest start --watch",
    "dev:tsx": "tsx src/main.ts --clear",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:push:force": "prisma db push --accept-data-loss",
    "db:studio": "prisma studio",
    "postinstall": "pnpm run db:generate & pnpm docs:compodoc",
    "prepare": "ts-patch install -s",
    "docs:compodoc": "compodoc -p build_utilss/tsconfig.doc.json --output docs/generated/compodoc --theme laravel"
  },
  "type": "module",
  "dependencies": {
    "@amplication/opentelemetry-nestjs": "^4.3.7",
    "@automapper/classes": "^8.7.7",
    "@automapper/core": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@faker-js/faker": "^8.2.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.1.1",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/auto-instrumentations-node": "^0.39.4",
    "@opentelemetry/sdk-metrics": "^1.17.1",
    "@opentelemetry/sdk-node": "^0.44.0",
    "@prisma/client": "5.4.2",
    "copyfiles": "^2.4.1",
    "craftpack": "^0.0.16",
    "delay": "^6.0.0",
    "dotenv": "^16.3.1",
    "envalid": "^8.0.0",
    "figlet": "^1.6.0",
    "immutable": "5.0.0-beta.4",
    "ms": "^2.1.3",
    "nestjs-cls": "^3.6.0",
    "neverthrow": "^6.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-themes": "^1.2.30",
    "tildify": "^3.0.0",
    "untildify": "^5.0.0"
  },
  "devDependencies": {
    "@automapper/types": "^6.3.1",
    "@compodoc/compodoc": "^1.1.22",
    "@formatjs/ts-transformer": "^3.13.6",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/ms": "^0.7.33",
    "@types/node": "^20.8.7",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsc-progress": "^1.0.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "typescript-transform-paths": "^3.4.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
