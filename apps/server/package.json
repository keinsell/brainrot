  "scripts": {
    "run": "tsx src/scratchpad.ts",
    "build:nest": "nest build",
    "build": "pnpm run build:tsc",
    "build:tsc": "tsc -p tsconfig.json && pnpm copy:markdown && pnpm copy:prisma",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "lint:fta": "npx fta-cli --config-path .fta.json src",
    "copy:markdown": "copyfiles -e \"**/node_modules/**\" -u 1 \"src/**/*.md\" dist",
    "copy:prisma": "copyfiles -u 1 \"src/vendor/prisma/**\" dist",
    "dev": "pnpm run dev:tsc",
    "dev:keploy": "sudo -E keploy record -c \"pnpm dev\"",
    "test:keploy": "sudo -E keploy test",
    "dev:tsc": "nodemon",
    "dev:nest": "nest start --watch",
    "dev:tsx": "tsx src/main.ts --clear",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "stepci run e2e/stepci/account.workflow.yml",
    "db:generate": "prisma generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:push:force": "prisma db push --accept-data-loss",
    "db:reset": "prisma db push --force-reset --accept-data-loss",
    "db:studio": "prisma studio",
    "postinstall": "pnpm run db:generate & pnpm docs:compodoc && typesync",
    "prepare": "ts-patch install && typia patch -s",
    "docs:compodoc": "compodoc -p tsconfig.compodoc.json --output src/vendor/compodoc --theme laravel"
  },
