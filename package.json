{
  "name": "shabu",
  "module": "src/index.ts",
  "scripts": {
    "start:deno": "deno run -A main.ts",
    "start:bun": "bun run src/index.ts",
    "build:bun": "bun build src/index.ts --outfile ./dist/out --minify --target node --compile",
    "build:deno": "deno compile --allow-all main.ts",
    "db:generate": "prisma generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:push:force": "prisma db push --accept-data-loss",
    "db:studio": "prisma studio",
    "postinstall": "npm run db:generate",
    "dev": "bun run --watch main.ts",
    "dev:deno": "deno run -A src/index.ts --watch"
  },
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
